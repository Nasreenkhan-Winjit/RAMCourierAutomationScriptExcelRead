<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Valid Login Scenario">
    <listeners>
        <listener class-name="utilities.listener.TestListener"/>
    </listeners>
    <test name="Check login and logout successfully">
        <classes>
            <class name="pages.LoginPageTest"/>
        </classes>
    </test>
    <test name="Test Consignment ExternalUser assigned MultipleBilledTos">
        <classes>
            <class name="pages.TestConsignmentExternalUser">
                <methods>
                    <include name="clickOnConsignment"></include>
                    <include name="clickOnAccordianAndSelectExternalUserCompany"></include>
                    <include name="externalUserConsignmentOutbound"></include>
                    <include name="externalUserConsignmentInbound"></include>
                </methods>
            </class>
        </classes>
    </test>

