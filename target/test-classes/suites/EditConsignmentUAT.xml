<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Verify the edit consignment data ">
    <listeners>
        <listener class-name="utilities.listener.TestListener"/>
    </listeners>
    <test name="Check login and logout successfully">
        <classes>
            <class name="pages.LoginPageTest"/>
        </classes>
    </test>
    <test name="Test Edit Consignment">
        <classes>
            <class name="pages.TestPageEditConsignmentUAT">
                <methods>
                    <include name="clickOnConsignment"></include>
                    <include name="clickOnAccordian"></include>
                    <include name="compareDataInEditConsignment"></include>
                </methods>
            </class>
        </classes>
    </test>



</suite>