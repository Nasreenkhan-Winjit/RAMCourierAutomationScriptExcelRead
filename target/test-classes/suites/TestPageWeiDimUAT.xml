<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Valid Login Scenario">
    <listeners>
        <listener class-name="utilities.listener.TestListener"/>
    </listeners>
    <test name="Check login and logout successfully">
        <classes>
            <class name="pages.LoginPageTest"/>
        </classes>
    </test>
    <test name="Test Weigh and Dimension page">
        <classes>
            <class name="pages.TestPageWeiDimUAT">
                <methods>
                    <include name="clickOnConsignment"></include>
                    <include name="clickOnAccordian"></include>
                    <include name="enterConsignmentIDOutbound"></include>
                    <include name="enterTrackingNumber"></include>
                    <include name="clickOnActionIcon"></include>
                    <include name="enterConsignmentIDInbound"></include>
                    <include name="enterTrackingNumberInbound"></include>
                    <include name="rMSPortalAndClickOnDimOutbound"></include>
                    <include name="rMSPortalAndClickOnDimInbound"></include>
                    <include name="verifyConsignmentIDOutboundOnWeiDim"></include>
                    <include name="verifyConsignmentIDInboundOnWeiDim"></include>
                    <include name="verifyParcelsInEditConsignmentOutbound"></include>
                    <include name="verifyParcelsInEditConsignmentInbound"></include>
                    <include name="verifyChangesOnWeiDimOutbound"></include>
                    <include name="verifyChangesWeiDimInbound"></include>
                    <include name="verifyChangesOnRMSPortalOutbound"></include>
                    <include name="verifyChangesOnRMSPortalInbound"></include>

                </methods>
            </class>
        </classes>
    </test>
</suite>