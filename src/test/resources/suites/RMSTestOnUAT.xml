<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name=" Verify create consignment on RMS ">
    <listeners>
        <listener class-name="utilities.listener.TestListener"/>
    </listeners>
    <test name="Check login and logout successfully">
        <classes>
            <class name="pages.LoginPageTest"/>
        </classes>
    </test>
    <test name="Test RMS portal for create consignment">
        <classes>
            <class name="pages.TestPageRMSUAT">
                <methods>
                    <include name="clickOnConsignment"></include>
                    <include name="clickOnRMSPortalAndVerifyConsignmentISCreated"></include>
                    <include name="embedScreenshotBYTES"></include>
                    <include name="verifyDataOnRMSEditConsignment"></include>
                </methods>
            </class>
        </classes>
    </test>
</suite>

























